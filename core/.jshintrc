{
    "asi": true,
    "camelcase": true,
    "eqeqeq": true,
    "eqnull": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxparams": 4,
    "maxdepth": 3,
    "maxstatements": 20,
    "maxcomplexity": 8,
    "maxlen": 120,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "trailing": true,

    "node": true,

    "predef": [
      "define", "module", "require",
      "before", "beforeEach", "afterEach", "describe", "it",
      "after", "window"
  ]

}
